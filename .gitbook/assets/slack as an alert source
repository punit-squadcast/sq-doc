                    if(window.location.pathname.slice(1,5) == 'docs' && window.location.pathname != '/docs/'){
                        document.getElementById('feedback').innerHTML = htm;
                        document.getElementById('yes-b').onclick = function(){
                            document.getElementById('form-block').style.display = 'block';
                            document.getElementById('yes-b').style.backgroundColor = '#3670fe';
                            document.getElementById('yes-b').style.color = 'white';
                            document.getElementById('no-b').style.backgroundColor = 'inherit';
                            document.getElementById('no-b').style.color = 'inherit';
                            document.getElementById('yes-d').style.display = 'none';
                            document.getElementById('yes-w').style.display = 'block';
                            document.getElementById('no-d').style.display = 'block';
                            document.getElementById('no-w').style.display = 'none';
                            document.getElementById('success').style.display = 'block';
                            document.getElementById('no-text').style.display = 'none';
                            document.getElementById('wf-form-docs').style.display = 'none';
                            document.getElementById('success').innerHTML = '<p style="text-align: center; margin-bottom: 0px;">Thank you for your feedback!</p>';
                        }
                        document.getElementById('no-b').onclick = function(){
                            document.getElementById('form-block').style.display = 'block';
                            document.getElementById('no-b').style.backgroundColor = '#3670fe';
                            document.getElementById('no-b').style.color = 'white';
                            document.getElementById('yes-b').style.backgroundColor = 'inherit';
                            document.getElementById('yes-b').style.color = 'inherit';
                            document.getElementById('yes-w').style.display = 'none';
                            document.getElementById('yes-d').style.display = 'block';
                            document.getElementById('no-w').style.display = 'block';
                            document.getElementById('no-d').style.display = 'none';
                            document.getElementById('success').style.display = 'none';
                            document.getElementById('no-text').style.display = 'block';
                            document.getElementById('wf-form-docs').style.display = 'block';
                        }
                        $('#wf-form-docs').submit(function(e){
                            document.getElementById('no-text').style.display = 'none';
                            document.getElementById('wf-form-docs').style.display = 'none';
                            try{
                                dataLayer.push({
                                    'event': 'submitEvent',
                                    'gtm.elementId': 'wf-form-docs',
                                    'eventMessage': document.getElementById('comment').value
                                });
                                document.getElementById('success').style.display = 'block';
                                document.getElementById('success').innerHTML = '<p style="text-align: center; margin-bottom: 0px;">Thank you! Your submission has been received!</p>';
                                e.preventDefault();
                            }catch(e){
                                document.getElementById('fail').style.display = 'block';
                            }
                        });
                    }
                </script>
