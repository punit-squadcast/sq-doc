                var btn1 = document.getElementById("sd1");
                var timezone1 = new Date().getTimezoneOffset() / -60;

                btn1.onclick = function(){
                    if(timezone1 >= -1){
                        document.getElementById("sd_anirudh").click();
                    }
                    if(timezone1 < -1){
                        document.getElementById("sd_nikil").click();
                    }
                }
                </script>
